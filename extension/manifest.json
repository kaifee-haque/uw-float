{
    "manifest_version": 2,
 
    "name": "UW Float", 
    "description": "UW Flow link previews",
    "version": "0.2",
    "author": "Kaifee Haque",

	"permissions": ["https://uwflow.com/graphql"],
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["content.js"],
        "matches": ["<all_urls>"],
        "run_at": "document_start"
    }],
	"web_accessible_resources": ["fonts/*"],
    "icons": {
        "16": "images/float_16.png",
        "32": "images/float_32.png",
        "64": "images/float_64.png",
        "128": "images/float_128.png"
    }
}
